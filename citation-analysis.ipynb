   "outputs": [],
   "source": [
    "ngram_1 = open_problems_df.copy()\n",
    "ngram_1[\"title_keywords\"] = ngram_1[\"titles\"].apply(lambda text: extract_keywords(text, ngram_range=(1, 1)))"
   ]
  },
  {
   "cell_type": "code",
