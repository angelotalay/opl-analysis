    "### Extract the key words from titles "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "4ab2d251-37f3-46ef-889a-c1746895cd85",
   "metadata": {},
   "outputs": [],
   "source": [
    "sentence_model = SentenceTransformer(\"pritamdeka/S-PubMedBert-MS-MARCO\")\n",
    "kw_model = KeyBERT(model=sentence_model)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "08321a3a-9b2e-4a21-9d90-dde1898c40e5",
   "metadata": {},
   "outputs": [],
   "source": [
    "ageing_stop_words = [\"ageing\", \"longevity\", \"aging\"]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "147b65b5-688c-4127-a680-de90cafd4f35",
   "metadata": {},
   "outputs": [],
   "source": [
    "def extract_keywords(text, ngram_range): \n",
    "    keywords = kw_model.extract_keywords(text, keyphrase_ngram_range=ngram_range, stop_words='english', use_mmr=True, diversity=0.5)\n",
    "    # Sort the keywords by score in descending order\n",
    "    keywords = sorted(keywords, key=lambda x: x[1], reverse=True)\n",
    "    # Filter keywords with score >= 0.85\n",
    "    return [k for k in keywords if k[0] not in ageing_stop_words]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "cb818d3c-7914-44f5-b871-4f79b5314d2a",
   "metadata": {},
   "outputs": [],
   "source": [
    "def extract_keywords_vectorizer(text, pipeline): \n",
    "    keywords = kw_model.extract_keywords(text, vectorizer=KeyphraseCountVectorizer(spacy_pipeline=pipeline))\n",
    "    keywords = sorted(keywords, key=lambda x: x[1], reverse=True)\n",
    "    # Filter keywords with score >= 0.85\n",
    "    return [k for k in keywords if k[0] not in ageing_stop_words]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
